BlockThis.prototype.getSanitizeAdInfo=function(ad_unit,ad_id){var ad=null,ad_info={};for(var x=0;x<SF.Ads.renderedAds.length;x++){ad=SF.Ads.renderedAds[x];if(ad.slot.getAdUnitPath().indexOf(ad_unit)!==-1){break;}}
